{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Identity": {
    "Password": {
      "RequireDigit": true,
      "RequireLowercase": true,
      "RequireUppercase": true,
      "RequireNonAlphanumeric": true,
      "RequiredLength": 6
    },
    "Lockout": {
      "DefaultLockoutTimeSpan": "0:05:00",
      "MaxFailedAccessAttempts": 5
    },
    "User": {
      "UserName": {
        "RequiredLength": 3,
        "RequireUniqueEmail": true
      },
      "Email": {
        "RequireUniqueEmail": true
      }
    }
  }
}
